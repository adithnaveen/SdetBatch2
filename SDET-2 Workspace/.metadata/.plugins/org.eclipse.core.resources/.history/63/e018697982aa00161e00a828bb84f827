package com.fannie.hibernate02;

public class Television extends Product {
	private int size;   // in inches 
	private String screenType; // LED/CRT/LCD
	private String resolution; // 1024x786, 2048 x 1400 
	
	public Television(){}
	
	
	

	public Television(int size, String screenType, String resolution) {
		this.size = size;
		this.screenType = screenType;
		this.resolution = resolution;
	}



	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public String getScreenType() {
		return screenType;
	}
	public void setScreenType(String screenType) {
		this.screenType = screenType;
	}
	public String getResolution() {
		return resolution;
	}
	public void setResolution(String resolution) {
		this.resolution = resolution;
	}
	
	
	
}
